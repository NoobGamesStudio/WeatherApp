<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:view="clr-namespace:WA_Front.View"
             xmlns:service="clr-namespace:WA_Front.Service"
             x:Class="WA_Front.View.MasterPage"
             x:Name="thisPage">
    <Grid RowDefinitions="*,Auto">
        <VerticalStackLayout Grid.Row="0" BindableLayout.ItemsSource="{Binding Views, Source={x:Reference thisPage}}">

        </VerticalStackLayout>
        <HorizontalStackLayout Grid.Row="1">
            <Label Text="co"/>
        </HorizontalStackLayout>
    </Grid>
</ContentPage>